.container{

}

.head{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding: 82rpx 0 50rpx;
  border-bottom: 1px #ececec solid;
  
}

.head .scan{
  width: 68rpx;
  height: 68rpx;
}
.head .data{
  width: 62rpx;
  height: 62rpx;
}

.head .title{
  font-size: 30rpx;
  color: #333333;
  text-align: center;
}

.movielist{
  padding: 0 30rpx;
}
.movielist .content{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 60rpx 0;
  border-bottom: 1px #ececec solid;
}
.movielist .content .img{
  width: 180rpx;
  height: 240rpx;
}
.movielist .content .right{
  width: 444rpx;
  margin-left: 50rpx;
}

.movielist .content .right .title{
  font-size: 34rpx;
  color: #333333;
}
.movielist .content .right .desc{
  font-size: 28rpx;
  color: #666666;
  letter-spacing: -0.41px;
  line-height: 42rpx;
  margin-top: 28rpx;
}
.movielist .content .right .btn{
  width: 120rpx;
  height: 50rpx;
  background: #EB582D;
  border-radius: 4px;
  text-align: center;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #F2F2F2;
  margin-top: 20rpx;
}


.data{

}
.data .top{
  padding: 82rpx 0 72rpx;
  text-align: center;
  border-bottom: 1px #ececec solid;
}
.data .top .unit{
  font-size: 28rpx;
  color: #999999;
  
}
.data .top .increase{
  font-size: 48rpx;
  color: #333333;
}
.data .content .nav{
  display: flex;
  justify-content: space-around;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #007B00;
  letter-spacing: -0.34px;
  padding: 76rpx 30rpx 24rpx;
  position: relative;
} 
.data .content .nav:after{
  content:"";
  display: block;
  height: 1px;
  position: absolute;
  left: 30rpx;
  bottom: 0;
  right: 30rpx;
  background-color: #ddd;
}
.data .content .datalist{
  font-size: 28rpx;
  color: #333333;
}
.data .content .datalist .item{
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}